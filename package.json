{
  "name": "hummus",
  "version": "0.1.0",
  "description": "An AngularJS 1.4, ES6, Sass, Angular Material, Material Design Icons, and Webpack starter app.",
  "scripts": {
    "clean": "rm -rf ./build && rm -rf ./dist",
    "build": "webpack --config webpack.build.js --bail -p",
    "dist": "./node_modules/http-server/bin/http-server -p 5555 dist",
    "dev": "node node_modules/.bin/webpack-dev-server --port=3333 --history-api-fallback --hot --inline --progress",
    "test": "karma start",
    "test:live": "karma start --auto-watch --no-single-run"
  },
